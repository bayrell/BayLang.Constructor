/*!
 *  BayLang Constructor
 */

namespace BayLang.Constructor.Frontend.Annotations;


interface WidgetSettingsInterface
{
	/**
	 * Returns widget name
	 */
	string getName();
	
	
	/**
	 * Returns short name
	 */
	string getShortName();
	
	
	/**
	 * Returns params
	 */
	Collection<string> getParams();
	
	
	/**
	 * Returns default template
	 */
	Dict<fn> getDefaultTemplate();
}