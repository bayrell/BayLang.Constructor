namespace BayLang.Constructor.Frontend.Editor.Styles;


class StyleItem
{
	string key = "";
	string value = "";
	
	
	/**
	 * Set value
	 */
	void setValue(string value)
	{
		this.value = value;
	}
	
	
	/**
	 * Returns value
	 */
	void getValue()
	{
		return this.value;
	}
}