/*!
 *  BayLang Constructor
 */

namespace BayLang.Constructor.Frontend.Editor.Styles;


class StyleItem
{
	string key = "";
	string value = "";
	
	
	/**
	 * Build value
	 */
	string buildValue() => this.value;
	
	
	/**
	 * Init value
	 */
	string initValue(string value)
	{
		this.value = value;
	}
	
	
	/**
	 * Set value
	 */
	void setValue(string value)
	{
		this.value = value;
	}
	
	
	/**
	 * Returns value
	 */
	void getValue() => this.value;
}